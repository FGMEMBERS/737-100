<PropertyList>

  <fx>

    <!-- Left engine -->
    <engine>
      <name>start0</name>
      <mode>once</mode>
      <path>Aircraft/737-100/Sounds/start.wav</path>
      <condition>
        <property>/controls/engines/engine[0]/starter</property>
      </condition>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        <min> 0.0 </min>
        <max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
    
    <engine>
      <name>whine0</name>
