 <eightyknots>
        <name>eightyknots</name>
        <mode>once</mode>
        <type>avionics</type>
        <path>Aircraft/737-100/Sounds/80kt.wav</path>
        <condition>
			<or>
			<property>/gear/gear[0]/wow</property>
			<property>/gear/gear[2]/wow</property>
			</or>	
			<greater-than>
				<property>/velocities/airspeed-kt</property>
				<value>80</value>
			</greater-than>
			<less-than>
				<property>/velocities/airspeed-kt</property>
				<value>88</value>
			</less-than>			
			</condition>	
