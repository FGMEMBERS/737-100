  <animation>
    <type>material</type>
    <object-name>panel</object-name>
    <emission>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>
  
  <animation> 
    <type>textranslate</type> 
    <object-name>panel</object-name> 
    <!--
    <condition>
      <equals>
        <property>/controls/lighting/panel-norm</property>
        <value>1</value>
      </equals>
    </condition>
    -->
    <property>/controls/lighting/panel-norm</property>
    <step> 1 </step>
    <factor> 2 </factor>
    <axis> 
      <x> -1 </x> 
      <y>  0 </y> 
      <z>  0 </z> 
    </axis> 
